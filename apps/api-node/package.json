{
  "name": "@cclabs/api-node",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf .turbo dist build *.tsbuildinfo",
    "start": "node dist/index.js",
    "dev": "tsnd --files --respawn --cls --exit-child src/cmd/http/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "TIMING=1 eslint \"src/**/*.ts?(x)\" && tsc --noEmit",
    "lint:fix": "cross-env TIMING=1 eslint \"src/**/*.ts?(x)\" --fix && tsc --noEmit",
    "test": "vitest run",
    "test:hook": "vitest run --changed HEAD~1",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:resolve": "prisma migrate resolve",
    "prisma:status": "prisma migrate status"
  },
  "keywords": [],
  "author": "Jonathan Robic <hello@jonathanrobic.fr>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.8.0",
    "debug": "4.3.4",
    "fastify": "4.10.2",
    "fastify-plugin": "4.4.0",
    "pino-pretty": "9.1.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@anatine/zod-mock": "3.6.0",
    "@cclabs/scripts": "workspace:*",
    "@faker-js/faker": "7.6.0",
    "@types/debug": "4.1.7",
    "@types/node": "16.18.3",
    "@vitest/coverage-c8": "0.26.0",
    "@vitest/ui": "0.26.0",
    "prisma": "4.8.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3",
    "vitest": "0.26.0"
  }
}
